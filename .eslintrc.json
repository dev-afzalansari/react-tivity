{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "error",
    "no-unused-vars": "error",
    "no-console": ["error", { "allow": ["warn"] }]
  }
}
